{
  "name": "Node.js & MySQL",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-azuretools.vscode-docker",
        "github.copilot",
        "rangav.vscode-thunder-client"
      ]
    }
  },
  "postCreateCommand": "cd /workspaces/haru_node_api && pnpm install",
  "remoteUser": "node",
  "mounts": [
    "type=bind,source=/tmp/ssh-XXXXXXZlj8GT/agent.23793,target=/ssh-agent"
  ],
  "remoteEnv": {
    "SSH_AUTH_SOCK": "/ssh-agent"
  }
}
